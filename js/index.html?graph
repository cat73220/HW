var line = [];
line[1] = [['2013-05-13 00:00:00', 0.028],];
line[11] = [['2013-05-13 00:00:00', 0.027],];
line[16] = [['2013-05-07 00:00:00', 0.057],];
line[17] = [['2013-05-13 00:00:00', 0.12],];
line[18] = [['2013-05-12 00:00:00', 0.08],];
line[19] = [['2013-05-12 00:00:00', 0.093],];
line[20] = [];

$(document).ready(function()
{
	$("#maps").show();
	$.jqplot.config.enablePlugins = true;
	
	var plot = $.jqplot('chart', [line[1],line[11],line[16],line[17],line[18],line[19],line[20]], {
		title: { text: '', show: false },
		grid:{ shadow: false },
		legend:{ show: false },
		seriesDefaults: {
			showMarker: true
		},
		seriesColors: ['#0000ff','#cc00ff','#006699','#663366','#996666','#669966','#cccc66'],
		series:[ { label:'札幌市', lineWidth:1, markerOptions:{style:'filledCircle'} },{ label:'網走市', lineWidth:1, markerOptions:{style:'square'} },{ label:'台北', lineWidth:1, markerOptions:{style:'circle'} },{ label:'香港', lineWidth:1, markerOptions:{style:'square'} },{ label:'北京', lineWidth:1, markerOptions:{style:'diamond'} },{ label:'上海', lineWidth:1, markerOptions:{style:'filledCircle'} },{ label:'ソウル', lineWidth:1, markerOptions:{style:'filledSquare'} }, ],
		cursor:{
			show: false
		},
		axes:{
			xaxis:{
				renderer: $.jqplot.DateAxisRenderer,
				rendererOptions:{tickRenderer:$.jqplot.CanvasAxisTickRenderer},
				min:'2013-05-07 00:00:00',
				max:'2013-05-13 00:00:00',
				tickInterval:'1 day',
				tickOptions:{
					formatString:'%m/%d', fontSize:'9pt', fontWeight:'normal', fontStretch:1
				}
			},
			yaxis:{
				min:0,
				max:0.5,
				tickInterval:0.02,
				tickOptions:{
					formatString:'%.3f',fontSize:'8pt', fontWeight:'normal', fontStretch:1
				}
			}
		}
	});
});


